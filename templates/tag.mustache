<!DOCTYPE html>
<html>
{{> head }}

<body>
	<div class="container wrapper tags">
        {{> header }}

        <h1>Entries tagged - "{{tag.name}}"</h1>
        <ul>
            {{#tag.posts}}
                <li><a href="{{site.base-url}}{{url}}">{{title}}</a></li>
            {{/tag.posts}}
        </ul>
	</div>

    {{> footer }}
</body>

</html>
